            <s xmlns:xs="http://www.w3.org/2001/XMLSchema" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:ns2="http://ws.binding.sca.tuscany.apache.org/" 
               xsi:type="xs:string">Fred
            </s>
        </axis2ns5:getGreetingsException>
